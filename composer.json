{
  "name": "torann/json-ld",
  "description": "Extremely simple JSON-LD markup generator.",
  "keywords": ["json-ld", "generator", "schema", "structured-data"],
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "torann@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.3",
    "phpstan/phpstan": "^0.12.14",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "JsonLd\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JsonLd\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },
  "archive" : {
    "exclude": [
      "/docs",
      "/tests"
    ]
  }
}
